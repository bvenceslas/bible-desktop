{
  "name": "bible",
  "version": "1.0.0",
  "description": "Bible For Desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack-dev-server --env.dev",
    "build": "webpack",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/oesukam/bible-electron",
  "keywords": [
    "bible",
    "book",
    "desktop"
  ],
  "author": "oesukam",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-reload": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "electron-packager": "^13.0.1"
  }
}
